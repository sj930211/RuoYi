<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.TScheduleMapper">
    
    <resultMap type="TSchedule" id="TScheduleResult">
        <result property="scheduleId"    column="schedule_id"    />
        <result property="project"    column="project"    />
        <result property="manager"    column="manager"    />
        <result property="applyStartDate"    column="apply_start_date"    />
        <result property="planEndDate"    column="plan_end_date"    />
    </resultMap>

    <sql id="selectTScheduleVo">
        select schedule_id, project, manager, apply_start_date, plan_end_date from t_schedule
    </sql>

    <select id="selectTScheduleList" parameterType="TSchedule" resultMap="TScheduleResult">
        <include refid="selectTScheduleVo"/>
        <where>  
            <if test="project != null  and project != ''"> and project = #{project}</if>
            <if test="manager != null  and manager != ''"> and manager = #{manager}</if>
            <if test="applyStartDate != null "> and apply_start_date = #{applyStartDate}</if>
            <if test="planEndDate != null "> and plan_end_date = #{planEndDate}</if>
        </where>
    </select>
    
    <select id="selectTScheduleByScheduleId" parameterType="String" resultMap="TScheduleResult">
        <include refid="selectTScheduleVo"/>
        where schedule_id = #{scheduleId}
    </select>
        
    <insert id="insertTSchedule" parameterType="TSchedule" useGeneratedKeys="true" keyProperty="scheduleId">
        insert into t_schedule
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="project != null and project != ''">project,</if>
            <if test="manager != null and manager != ''">manager,</if>
            <if test="applyStartDate != null">apply_start_date,</if>
            <if test="planEndDate != null">plan_end_date,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="project != null and project != ''">#{project},</if>
            <if test="manager != null and manager != ''">#{manager},</if>
            <if test="applyStartDate != null">#{applyStartDate},</if>
            <if test="planEndDate != null">#{planEndDate},</if>
         </trim>
    </insert>

    <update id="updateTSchedule" parameterType="TSchedule">
        update t_schedule
        <trim prefix="SET" suffixOverrides=",">
            <if test="project != null and project != ''">project = #{project},</if>
            <if test="manager != null and manager != ''">manager = #{manager},</if>
            <if test="applyStartDate != null">apply_start_date = #{applyStartDate},</if>
            <if test="planEndDate != null">plan_end_date = #{planEndDate},</if>
        </trim>
        where schedule_id = #{scheduleId}
    </update>

    <delete id="deleteTScheduleByScheduleId" parameterType="String">
        delete from t_schedule where schedule_id = #{scheduleId}
    </delete>

    <delete id="deleteTScheduleByScheduleIds" parameterType="String">
        delete from t_schedule where schedule_id in 
        <foreach item="scheduleId" collection="array" open="(" separator="," close=")">
            #{scheduleId}
        </foreach>
    </delete>

</mapper>